<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<configuration doc:name="Configuration" doc:id="398fa63f-72d6-4dbc-a191-86c0abe80d00" defaultErrorHandler-ref="globalError_Handler" />
	<error-handler name="globalError_Handler" doc:id="be9d2109-7b3f-4d27-8e9b-43b815859fba" >
		<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="a7967989-cdea-4615-9407-6d66f8780a1a" type="EXPRESSION">
			<ee:transform doc:name="Transform Message" doc:id="df02d361-b053-4cc0-9a3e-3795b2cba6d6" >
				<ee:message >
					<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
	message: "Mule Expression Error"
}]]></ee:set-payload>
				</ee:message>
			</ee:transform>
		</on-error-continue>
	</error-handler>
</mule>
